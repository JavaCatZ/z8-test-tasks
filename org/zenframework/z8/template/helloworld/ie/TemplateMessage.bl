import org.zenframework.z8.ie.DataMessage;
import org.zenframework.z8.template.helloworld.model.HelloWorld;

public class TemplateMessage extends DataMessage {

	virtual protected void afterImport() {
		HelloWorld hello;
		guid[] ids = getRecords(hello);
		for (guid id : ids) {
			hello.greeting = "Received at " + date.now().toString("HH:mm:ss");
			hello.update(id);
		}
	}

}